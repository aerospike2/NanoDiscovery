{
    "modules": ["commanduino","triconts","pH", "spectrometers"],
    "commanduino": {
        "ios": [
            {
                "type": "serial",
                "port": "COM8"
            },
            {
                "type": "serial",
                "port": "COM9"
                }],
        "devices": {
            "sample": {
                "command_id": "X",
                "config": {
                    "reverted_switch": true,
                    "reverted_direction": false,
                    "enabled_acceleration": false,
                    "speed": 20000,
                    "homing_speed": 20000,
                    "acceleration": 2000
                }
            },
            "ph_horz": {
                "command_id": "Y",
                "config": {
                    "reverted_switch": true,
                    "reverted_direction": false,
                    "enabled_acceleration": false,
                    "speed": 20000,
                    "max_speed": 20000,
                    "homing_speed": 20000,
                    "acceleration": 2000
                }
            },
            "nil": {
                "command_id": "Z",
                "config": {
                    "reverted_switch": true,
                    "reverted_direction": false,
                    "enabled_acceleration": false,
                    "speed": 12000,
                    "max_speed": 12000,
                    "homing_speed": 12000,
                    "acceleration": 2000
                }
            },
            "wheel": {
                "command_id": "E0",
                "config": {
                    "reverted_switch": true,
                    "reverted_direction": false,
                    "enabled_acceleration": false,
                    "speed": 5000,
                    "max_speed": 12000,
                    "homing_speed": 12000,
                    "acceleration": 2000
                }
            },
            "ph_vert": {
                "command_id": "E1",
                "config": {
                    "speed": 20000,
                    "homing_speed": 20000,
                    "max_speed": 50000,
                    "reverted_switch": true,
                    "reverted_direction": false,
                    "enabled_acceleration": false
                }
            },
            
            "seed_v": {
                "command_id": "X2",
                "config": {
                    "reverted_switch": false,
                    "reverted_direction": false,
                    "enabled_acceleration": false,
                    "speed": 300000,
                    "max_speed": 300000,
                    "homing_speed": 300000
                }
            },
            "seed_h": {
                "command_id": "Y2",
                "config": {
                    "reverted_switch": false,
                    "reverted_direction": false,
                    "enabled_acceleration": false,
                    "speed": 300000,
                    "max_speed": 300000,
                    "homing_speed": 300000
                }
            },
            "ring": {
                "command_id": "ring"
            },
            "wash_fan": {
                "command_id": "wash"
            }
        }
    },
    "triconts": {

        "default": {
            "volume": 5,
            "micro_step_mode": 2,
            "top_velocity": 8000,
            "initialize_valve_position": "I"
        },
        "groups": {
            "cleaning": ["acid","water"],
            "spheres": ["gold", "surfactant", "reductant"],
            "rods": ["gold", "surfactant", "reductant", "silver", "seeds","acid"]
        },
        "hubs":[
            {
                "io": {
                    "port": "COM6",
                    "baudrate": 9600,
                    "timeout": 1
                },
                "pumps": {
                    "sample": {
                        "switch": "0",
                        "initialize_valve_position": "1",
                        "top_velocity": 5000
                    },
                    "water": {
                        "switch": "1",
                        "initialize_valve_position": "1",
                        "top_velocity": 5000
                    },
                    "water_reagent": {
                        "switch": "2",
                        "volume": 1.0,
                        "top_velocity": 5000
                    },
                    "surfactant": {
                        "switch": "3",
                        "volume": 5.0,
                        "top_velocity": 5000
                    },
                    "reductant": {
                        "switch": "4",
                        "volume": 0.5
                    },
                    "seeds": {
                        "switch": "5",
                        "volume": 0.5
                    },
                    "gold": {
                        "switch": "6",
                        "volume": 2.5
                    },
                    "silver": {
                        "switch": "7",
                        "volume": 2.5
                    },
                    "probe": {
                        "switch": "8",
                        "volume": 5
                    },
                    "kcl": {
                        "switch": "9"
                    },
                    "pH_acid": {
                        "switch": "A",
                        "volume": 0.25
                    },
                    "pH_base": {
                        "switch": "B",
                        "volume": 0.5
                    },
                    "CTAC": {
                        "switch": "C",
                        "volume": 5
                    }
                }
            },
            {
                "io":{
                    "port": "COM7",
                    "baudrate": 9600,
                    "timeout": 1
                },
                "pumps":{
                    "wheel_seed": {
                        "switch": "0",
                        "volume": 1
                    },
                    "HCl": {
                        "switch": "1",
                        "volume": 2.5
                    },
                    "hydroquinone": {
                        "switch": "2",
                        "volume": 1.0
                    }
                }
            }]},
    "camera": {
        "device": 0,
        "control_config": {
        "brightness": 133,
        "contrast": 5,
        "saturation": 83,
        "white_balance_temperature_auto": 1,
        "power_line_frequency": 1,
        "sharpness": 50,
        "backlight_compensation": 0,
        "exposure_auto": 3,
        "focus_auto": 1,
        "focus_absolute": 28,
        "zoom_absolute": 0
        }
    },
    "pH_calibrations": {
        "pH4": 400.0,
        "pH7": 700.0,
        "pH10": 1000.0
    },
    "spectrometers":{
        "uv":{
            "integration_time":0.01
        },
        "ir":{
            "integration_time":0.15
        }
    },
    "data_path": "./data/"
}
