Cell[BoxData[
 RowBox[{
  RowBox[{"createDipoles", "[", 
   RowBox[{
   "coordData_", ",", " ", "coordNumData_", ",", " ", "baseDirectory_"}], 
   "]"}], " ", ":=", " ", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "xRange", ",", " ", "yRange", ",", " ", "zRange", ",", " ", "nDipolesX", 
      ",", " ", "nDipolesY", ",", " ", "nDipolesZ", ",", " ", "dipoleList", 
      ",", " ", "dipoleListF", ",", "coordDataReal", ",", "coordNumList", ",",
       "SurfaceAtoms", ",", "RegionMesh"}], "}"}], ",", "\n", "    ", 
    RowBox[{"(*", " ", 
     RowBox[{
     "Creates", " ", "coordinates", " ", "of", " ", "the", " ", "dipoles", 
      " ", "by", " ", "creating", " ", "continuum", " ", "mesh", " ", "for", 
      " ", "DDA", " ", "calculations"}], " ", "*)"}], "\n", "    ", 
    RowBox[{
     RowBox[{"coordDataReal", " ", "=", " ", 
      RowBox[{"coordData", "*", "latticeC"}]}], ";", " ", 
     RowBox[{"(*", " ", 
      RowBox[{
      "Converting", " ", "atomic", " ", "coordinates", " ", "into", " ", 
       "nm"}], " ", "*)"}], "\n", "    ", 
     RowBox[{"coordNumList", " ", "=", " ", 
      RowBox[{"Flatten", "@", 
       RowBox[{"(", 
        RowBox[{"IntegerPart", "/@", "coordNumData"}], ")"}]}]}], ";", "\n", 
     "    ", 
     RowBox[{"SurfaceAtoms", " ", "=", " ", 
      RowBox[{"Pick", "[", 
       RowBox[{"coordDataReal", ",", " ", "coordNumList", ",", " ", 
        RowBox[{"Except", "[", 
         RowBox[{"12", ",", "_Integer"}], "]"}]}], "]"}]}], ";", "\n", "    ", 
     RowBox[{"RegionMesh", " ", "=", " ", 
      RowBox[{"DelaunayMesh", "[", "SurfaceAtoms", "]"}]}], ";", "\n", "    ", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"xRange", ",", " ", "yRange", ",", " ", "zRange"}], "}"}], " ",
       "=", " ", 
      RowBox[{"N", "@", 
       RowBox[{"RegionBounds", "[", "RegionMesh", "]"}]}]}], ";", "\n", 
     "    ", 
     RowBox[{"nDipolesX", " ", "=", " ", 
      RowBox[{
       RowBox[{"First", "@", 
        RowBox[{"Round", "[", 
         RowBox[{
          RowBox[{"(", 
           RowBox[{"1", "/", "dLength"}], ")"}], "*", 
          RowBox[{"Differences", "@", "xRange"}]}], "]"}]}], "+", "3"}]}], 
     ";", "\n", "    ", 
     RowBox[{"nDipolesY", " ", "=", " ", 
      RowBox[{
       RowBox[{"First", "@", 
        RowBox[{"Round", "[", 
         RowBox[{
          RowBox[{"(", 
           RowBox[{"1", "/", "dLength"}], ")"}], "*", 
          RowBox[{"Differences", "@", "yRange"}]}], "]"}]}], "+", "3"}]}], 
     ";", "\n", "    ", 
     RowBox[{"nDipolesZ", " ", "=", " ", 
      RowBox[{
       RowBox[{"First", "@", 
        RowBox[{"Round", "[", 
         RowBox[{
          RowBox[{"(", 
           RowBox[{"1", "/", "dLength"}], ")"}], "*", 
          RowBox[{"Differences", "@", "zRange"}]}], "]"}]}], "+", "3"}]}], 
     ";", "\n", "    ", 
     RowBox[{"dipoleList", " ", "=", " ", 
      RowBox[{"Flatten", "[", 
       RowBox[{
        RowBox[{"Table", "[", 
         RowBox[{
          RowBox[{"{", 
           RowBox[{
            RowBox[{
             RowBox[{"xRange", "[", 
              RowBox[{"[", "1", "]"}], "]"}], "+", " ", 
             RowBox[{"xD", "*", "dLength"}]}], ",", " ", "\n", 
            "                                ", 
            RowBox[{
             RowBox[{"yRange", "[", 
              RowBox[{"[", "1", "]"}], "]"}], "+", " ", 
             RowBox[{"yD", "*", "dLength"}]}], ",", "\n", 
            "                                ", 
            RowBox[{
             RowBox[{"zRange", "[", 
              RowBox[{"[", "1", "]"}], "]"}], " ", "+", " ", 
             RowBox[{"zD", "*", "dLength"}]}]}], "}"}], ",", "\n", 
          "                                ", 
          RowBox[{"{", 
           RowBox[{"xD", ",", "0", ",", "nDipolesX"}], "}"}], ",", " ", 
          RowBox[{"{", 
           RowBox[{"yD", ",", "0", ",", "nDipolesY"}], "}"}], ",", " ", 
          RowBox[{"{", 
           RowBox[{"zD", ",", "0", ",", "nDipolesZ"}], "}"}]}], "]"}], ",", 
        "2"}], "]"}]}], ";", "\n", "    ", 
     RowBox[{"dipoleListF", " ", "=", " ", 
      RowBox[{
       RowBox[{
        RowBox[{"Pick", "[", 
         RowBox[{
          RowBox[{"#", "[", 
           RowBox[{"[", "1", "]"}], "]"}], ",", " ", 
          RowBox[{"#", "[", 
           RowBox[{"[", "2", "]"}], "]"}]}], "]"}], "&"}], "/@", 
       RowBox[{"Evaluate", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"{", 
           RowBox[{"#", ",", " ", 
            RowBox[{"RegionMember", "[", 
             RowBox[{"RegionMesh", ",", " ", "#"}], "]"}]}], "}"}], "&"}], "/@",
          "dipoleList"}], "]"}]}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"dipoleListF", " ", "=", " ", 
      FractionBox["dipoleListF", "dLength"]}], ";", "\n", "    ", 
     RowBox[{"Export", "[", 
      RowBox[{
       RowBox[{
       "baseDirectory", " ", "<>", " ", "\"\<//dipoleList_\>\"", " ", "<>", 
        " ", 
        RowBox[{"ToString", "[", "dLength", "]"}], "<>", "\"\<_d_le_\>\"", 
        " ", "<>", " ", "\"\<.csv\>\""}], ",", " ", "dipoleListF", ",", 
       "\"\<CSV\>\""}], "]"}], ";", "\n", "    ", 
     RowBox[{"Return", "[", 
      RowBox[{"Length", "@", "dipoleListF"}], "]"}]}]}], "]"}]}]], "Input",
